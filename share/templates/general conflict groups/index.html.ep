<link rel="stylesheet" href="/css/collectionIndex.css" />

<div class="collection-list">
  <ul class="collection-grid">
    <% if ($items && ref $items eq 'HASH'  ) { %>
      <% foreach my $key (sort {$items->{$a}->name cmp $items->{$b}->name} keys %{ $items} ) { %>
        <li class="collection-item">
          <a href="/Reference/Conflict Groups/<%= $key %>/">
            <%= $items->{$key}->name %>
          </a>
        </li>
      <% } %>
    <% } %>
  </ul>
</div>
